<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<script>
    function alertSuccess(message){
        return Swal.fire({
            icon: 'success',
            title: 'Success!',
            text: message,
            timer: 1500, // time in milliseconds (3000ms = 3 seconds)
            timerProgressBar: true,
            showConfirmButton: true,
        });
    }

    function alertError(message){
        return Swal.fire({
            icon: 'error',
            title: 'Error!',
            text: message,
            timerProgressBar: true,
            showConfirmButton: true,
        });
    }

</script>
<% if (successMessage) { %>
    <script>
          alertSuccess('<%= successMessage %>');
    </script>
<% } %>

<% if (errorMessage) { %>
    <script>
          alertError('<%= errorMessage %>');
    </script>
<% } %>

